{
  "name": "foodkeys-weblog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed": "npx tsx prisma/seed.ts",
    "db:reset": "prisma migrate reset",
    "docker:build": "docker-compose -f deploy/docker-compose.yml build",
    "docker:up": "docker-compose -f deploy/docker-compose.yml up -d",
    "docker:down": "docker-compose -f deploy/docker-compose.yml down",
    "docker:logs": "docker-compose -f deploy/docker-compose.yml logs -f",
    "docker:migrate": "docker-compose -f deploy/docker-compose.yml --profile migrate up migrate",
    "docker:seed": "docker-compose -f deploy/docker-compose.yml --profile seed up seed",
    "docker:dev": "docker-compose -f deploy/docker-compose.dev.yml up -d",
    "docker:dev:down": "docker-compose -f deploy/docker-compose.dev.yml down",
    "docker:dev:logs": "docker-compose -f deploy/docker-compose.dev.yml logs -f",
    "docker:dev:pgadmin": "docker-compose -f deploy/docker-compose.dev.yml --profile tools up -d pgadmin",
    "docker:prod": "docker-compose -f deploy/docker-compose.prod.yml up -d",
    "docker:prod:down": "docker-compose -f deploy/docker-compose.prod.yml down",
    "setup": "npm install && npm run docker:dev && npm run db:generate && npm run db:push && npm run db:seed"
  },
  "dependencies": {
    "next": "^14.2.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "@prisma/client": "^5.14.0",
    "otplib": "^12.0.1",
    "framer-motion": "^11.2.0",
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.3.0",
    "class-variance-authority": "^0.7.0",
    "lucide-react": "^0.378.0",
    "slugify": "^1.6.6",
    "date-fns": "^3.6.0",
    "sharp": "^0.33.4",
    "next-themes": "^0.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.12.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.0",
    "postcss": "^8.4.38",
    "prisma": "^5.14.0",
    "tailwindcss": "^3.4.3",
    "@tailwindcss/typography": "^0.5.13",
    "typescript": "^5.4.5",
    "tsx": "^4.10.0"
  }
}
